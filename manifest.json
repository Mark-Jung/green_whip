{
 "manifest_version": 2, 

  "name": "fkNU",
  "description": "This extension creates a popup window.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
